<h1>New song</h1>

<% form_for(@song) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :composer %><br />
    <%= f.text_field :composer %>
  </p>
  <p>
    <%= f.label :artist_or_group %><br />
    <%= f.text_field :artist_or_group %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', songs_path %>
